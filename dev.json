{
    "linter": {
        "rules": {
            "a11y": {
                "useSemanticElements": "warn",
                "noSvgWithoutTitle": "warn",
                "noLabelWithoutControl": "error",
                "noNoninteractiveElementToInteractiveRole": "error",
                "noNoninteractiveTabindex": "error",
                "noRedundantRoles": "error",
                "useAltText": "error",
                "useButtonType": "error",
                "useFocusableInteractive": "error",
                "useKeyWithClickEvents": "error",
                "useKeyWithMouseEvents": "error",
                "useValidAnchor": "error",
                "useValidAriaProps": "error",
                "useValidAriaRole": "error"
            },
            "complexity": {
                "noForEach": "info",
                "useOptionalChain": "info",
                "noBannedTypes": "info",
                "useLiteralKeys": "info",
                "noExcessiveCognitiveComplexity": "info",
                "noUselessStringConcat": "info",
                "noUselessUndefinedInitialization": "info",
                "noVoid": "info",
                "useDateNow": "warn",
                "useSimplifiedLogicExpression": "warn",
                "noExtraBooleanCast": "error",
                "noStaticOnlyClass": "error",
                "noUselessCatch": "error",
                "noUselessConstructor": "error",
                "noUselessEmptyExport": "error",
                "noUselessFragments": "error",
                "noUselessLoneBlockStatements": "error",
                "noUselessRename": "error",
                "noUselessSwitchCase": "error",
                "noUselessTernary": "error",
                "useArrowFunction": "error",
                "useFlatMap": "error",
                "useRegexLiterals": "error"
            },
            "correctness": {
                "noNodejsModules": "info",
                "noUndeclaredDependencies": "info",
                "noUndeclaredVariables": "info",
                "noUnusedFunctionParameters": "info",
                "noUnusedImports": "info",
                "noUnusedVariables": "info",
                "useHookAtTopLevel": "info",
                "useImportExtensions": "info",
                "useExhaustiveDependencies": "warn",
                "noChildrenProp": "error",
                "noConstantCondition": "error",
                "noEmptyPattern": "error",
                "noFlatMapIdentity": "error",
                "noInnerDeclarations": "error",
                "noSwitchDeclarations": "error",
                "noUnsafeOptionalChaining": "error",
                "noVoidElementsWithChildren": "error",
                "useJsxKeyInIterable": "error"
            },
            "nursery": {
                "noCommonJs": "info",
                "noDocumentCookie": "info",
                "noDynamicNamespaceImportAccess": "info",
                "noEnum": "info",
                "noExportedImports": "info",
                "noImgElement": "info",
                "noIrregularWhitespace": "info",
                "noNestedTernary": "info",
                "noOctalEscape": "info",
                "noProcessEnv": "info",
                "noSecrets": "info",
                "noStaticElementInteractions": "info",
                "noTemplateCurlyInString": "info",
                "useAtIndex": "info",
                "useCollapsedIf": "info",
                "useComponentExportOnlyModules": "info",
                "useConsistentCurlyBraces": "info",
                "useGuardForIn": "info",
                "useImportRestrictions": "info",
                "useSortedClasses": "warn",
                "useExplicitType": "info"
            },
            "performance": {
                "noBarrelFile": "info",
                "noReExportAll": "info",
                "useTopLevelRegex": "info",
                "noAccumulatingSpread": "error",
                "noDelete": "error"
            },
            "security": {
                "noDangerouslySetInnerHtml": "error"
            },
            "style": {
                "noDefaultExport": "info",
                "noDoneCallback": "info",
                "noImplicitBoolean": "info",
                "noNamespace": "info",
                "noNamespaceImport": "info",
                "noNegationElse": "info",
                "noParameterProperties": "info",
                "noShoutyConstants": "info",
                "noYodaExpression": "info",
                "useBlockStatements": "info",
                "useCollapsedElseIf": "info",
                "useConsistentArrayType": "info",
                "useConsistentBuiltinInstantiation": "info",
                "useDefaultSwitchClause": "info",
                "useExplicitLengthCheck": "info",
                "useFilenamingConvention": "info",
                "useForOf": "info",
                "useFragmentSyntax": "info",
                "useNamingConvention": "info",
                "useShorthandArrayType": "info",
                "useShorthandAssign": "info",
                "useSingleCaseStatement": "info",
                "useThrowNewError": "info",
                "useThrowOnlyError": "info",
                "useImportType": "warn",
                "noUselessElse": "warn",
                "noUnusedTemplateLiteral": "warn",
                "useConst": "warn",
                "useTemplate": "warn",
                "useSelfClosingElements": "warn",
                "useNodejsImportProtocol": "warn",
                "useNumberNamespace": "warn",
                "noCommaOperator": "error",
                "noInferrableTypes": "error",
                "noNonNullAssertion": "error",
                "noParameterAssign": "error",
                "noVar": "error",
                "useDefaultParameterLast": "error",
                "useEnumInitializers": "error",
                "useExponentiationOperator": "error",
                "useExportType": "error",
                "useShorthandFunctionType": "error",
                "useSingleVarDeclarator": "error"
            },
            "suspicious": {
                "noConsole": "info",
                "noConsoleLog": "info",
                "noEmptyBlockStatements": "info",
                "noEvolvingTypes": "info",
                "noMisplacedAssertion": "info",
                "noReactSpecificProps": "info",
                "noSkippedTests": "info",
                "useAwait": "info",
                "useErrorMessage": "info",
                "noExplicitAny": "warn",
                "noDoubleEquals": "warn",
                "noArrayIndexKey": "warn",
                "noApproximativeNumericConstant": "error",
                "noAssignInExpressions": "error",
                "noAsyncPromiseExecutor": "error",
                "noConfusingVoidType": "error",
                "noConstEnum": "error",
                "noDuplicateFontNames": "error",
                "noDuplicateObjectKeys": "error",
                "noEmptyInterface": "error",
                "noExportsInTest": "error",
                "noFocusedTests": "error",
                "noGlobalIsNan": "error",
                "noImplicitAnyLet": "error",
                "noPrototypeBuiltins": "error",
                "noRedeclare": "error",
                "noRedundantUseStrict": "error",
                "noShadowRestrictedNames": "error",
                "noThenProperty": "error",
                "useDefaultSwitchClauseLast": "error",
                "useIsArray": "error"
            }
        }
    }
}